{
  "name": "cmake-explicit-glob",
  "displayName": "CMake Explicit Glob",
  "description": "Explicitly glob for cmake (without using file(GLOB ...))",
  "version": "0.0.1",
  "publisher": "kruumy",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/kruumy/cmake-explicit-glob.git"
  },
  "engines": {
    "vscode": "^1.86.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": "cmake",
        "path": "./snippets/cmake.json"
      }
    ]
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "activationEvents": [
    "onLanguage:cmake"
  ],
  "main": "./src/extension.js",
  "devDependencies": {
    "@types/vscode": "^1.86.0"
  },
  "dependencies": {
    "glob": "^13.0.1"
  }
}